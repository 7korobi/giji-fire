{
  "name": "giji-fire",
  "version": "1.0.0",
  "description": "人狼議事",
  "author": "7korobi@gmail.com",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "jest": "jest -u --config app/config/jest.js --rootDir .",
    "test": "run-s jest",
    "func": "run-s copy:func deploy:func",
    "precommit": "yarn",
    "generate": "run-p generate:*",
    "generate:nuxt": "nuxt generate",
    "copy": "run-s copy_all dist:copy",
    "copy_all": "run-p -n copy:*",
    "copy:sass": "ls app/styl/*.use.sass | xargs -L 1 node-sass --precision 1 --output-style compressed -o static/css/",
    "copy:stylus": "ls app/styl/*.styl.use | xargs stylus --compress --out static/css/",
    "copy:portrate": "rsync -avz ../../*/web_work/images/portrate/* static/images/portrate/.",
    "copy:func": "webpack --config app/config/webpack/index-functions.coffee",
    "dist:copy": "rsync -avz static/* dist/.",
    "dist:brotli": "find dist -type f -not -name \"*.br\" | grep -v \"dist/font\" | xargs -L1  brotli -Z -f -k",
    "deploy": "run-s generate:nuxt deploy:img backup",
    "deploy:test": "run-p -n deploy:host deploy:aws",
    "deploy:live": "run-s -n dist:brotli deploy:rsync deploy:aws deploy:aws_img backup",
    "deploy:styl": "run-s copy:stylus dist:copy deploy:test",
    "deploy:sow": "run-s copy:sow dist:copy deploy:test",
    "deploy:img": "run-s copy:portrate dist:copy deploy:test",
    "deploy:copy": "run-s copy dist:copy",
    "deploy:aws_img": "aws s3 sync dist/images s3://giji-assets/images --acl public-read --exclude '*.html'",
    "deploy:aws": "aws s3 sync dist s3://giji-assets/fire --acl public-read --exclude '*.html'",
    "deploy:rsync": "rsync -avz -e 'ssh -i ../.ssh/id_rsa' dist 7korobi@giji.f5.si:/home/7korobi/giji-fire/.",
    "deploy:all": "firebase deploy",
    "deploy:host": "firebase deploy --only hosting",
    "deploy:func": "firebase deploy --only functions",
    "backup": "run-p -n backup:*",
    "backup:rsync-front": "rsync -avz -e 'ssh -i ../.ssh/id_rsa'          7korobi@giji.f5.si:/home/7korobi/*.zip ../backup/.",
    "backup:rsync-mongo": "rsync -avz -e 'ssh -i ../.ssh/id_rsa -p 20000' 7korobi@giji-api.duckdns.org:/home/7korobi/*.zip ../backup/.",
    "backup:rsync-data": "rsync -avz -e 'ssh -i ../.ssh/id_rsa -p 10000' 7korobi@giji-api.duckdns.org:/home/7korobi/*.zip ../backup/.",
    "backup:rsync-maple": "rsync -avz -e 'ssh -i ../.ssh/id_rsa -p 60000' 7korobi@giji-api.duckdns.org:/home/7korobi/*.zip ../backup/.",
    "backup:競合": "find .. -name '*競合*' | ruby -ne 'puts %q[rm \"] + $_.chomp + %q[\"]' > dropbox.error.txt",
    "backup-local": "run-p -n backup-local:*",
    "backup-local:rsync-mongo": "rsync -avz -e 'ssh -i ../.ssh/id_rsa -p 20000' 7korobi@192.168.0.200:/home/7korobi/*.zip ../backup/.",
    "backup-local:rsync-data": "rsync -avz -e 'ssh -i ../.ssh/id_rsa -p 10000' 7korobi@192.168.0.100:/home/7korobi/*.zip ../backup/.",
    "backup-local:rsync-maple": "rsync -avz -e 'ssh -i ../.ssh/id_rsa -p 60000' 7korobi@192.168.0.60:/home/7korobi/*.zip  ../backup/.",
    "backup-local:競合": "find .. -name '*競合*' | ruby -ne 'puts %q[rm \"] + $_.chomp + %q[\"]' > dropbox.error.txt",
    "g": "run-p -n g:*",
    "g:msys": "pacman -S openssh rsync",
    "g:npm": "npm install -g aws-sdk firebase-tools npm-check-updates typesync"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@nuxtjs/google-analytics": "^2.2.2",
    "@nuxtjs/pwa": "^3.0.0-beta.14",
    "animejs": "^3.1.0",
    "core-js": "2.6.9",
    "cross-env": "^6.0.3",
    "fast-diff": "^1.2.0",
    "firebase-admin": "8.8.0",
    "firebase-functions": "3.3.0",
    "immutable": "^4.0.0-rc.12",
    "normalize-url": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^2.10.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "slate": "^0.47.9",
    "slate-html-serializer": "^0.8.11",
    "slate-react": "^0.22.10",
    "trix": "^1.2.2",
    "vue-petit-store": "^0.9.8",
    "vue-toasted": "^1.1.27",
    "vuera": "^0.2.6"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-modules-commonjs": "^7.7.4",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.4.1",
    "html-loader": "^0.5.5",
    "jest": "^24.9.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.1",
    "pug": "^2.0.4",
    "pug-jest": "^1.0.1",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "8.0.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "vue-jest": "^3.0.5",
    "webpack-cli": "^3.3.10",
    "yaml-jest": "^1.0.5",
    "yaml-loader": "^0.5.0"
  }
}
