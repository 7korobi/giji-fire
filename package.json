{
  "name": "giji-fire",
  "version": "1.0.0",
  "description": "人狼議事",
  "author": "7korobi@gmail.com",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "jest": "jest -u --config app/config/jest.js --rootDir .",
    "test": "run-s jest",
    "func": "run-s make:func deploy:func",
    "precommit": "yarn",
    "generate": "run-p generate:*",
    "generate:nuxt": "nuxt generate",
    "make": "run-s make_all dist:make",
    "make_all": "run-p -n make:*",
    "make:sass": "rsync -av --delete ../giji/app/css/ static/css/",
    "make:image": "rsync -av --delete ../giji/app/images/ static/images/",
    "make:func": "webpack --config app/config/webpack/index-functions.coffee",
    "dist:make": "rsync -avz static/* dist/.",
    "dist:brotli": "find dist -type f -not -name \"*.br\" | grep -v \"dist/font\" | xargs -L1  brotli -Z -f -k",
    "deploy": "run-s generate:nuxt deploy:img",
    "deploy:test": "run-p -n deploy:host deploy:aws",
    "deploy:live": "run-s -n dist:brotli deploy:rsync deploy:aws deploy:aws_img",
    "deploy:sass": "run-s make:sass dist:make deploy:test",
    "deploy:sow": "run-s make:sow dist:make deploy:test",
    "deploy:img": "run-s make:image dist:make deploy:test",
    "deploy:make": "run-s make dist:make",
    "deploy:aws_img": "aws s3 sync dist/images s3://giji-assets/images --acl public-read --exclude '*.html' --profile 7korobi",
    "deploy:aws": "aws s3 sync dist s3://giji-assets/fire --acl public-read --exclude '*.html' --profile 7korobi",
    "deploy:rsync": "rsync -avz -e 'ssh -i ../.ssh/id_rsa' dist 7korobi@giji.f5.si:/home/7korobi/giji-fire/.",
    "deploy:all": "firebase deploy",
    "deploy:host": "firebase deploy --only hosting",
    "deploy:func": "firebase deploy --only functions",
    "g": "run-p -n g:*",
    "g:msys": "pacman -S openssh rsync",
    "g:npm": "npm install -g aws-sdk firebase-tools npm-check-updates typesync  node-gyp grpc immutable memorystream minimatch nice-try path-key read-pkg shell-quote string.prototype.padend which"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/pwa": "^3.2.2",
    "@types/lodash": "^4.14.162",
    "animejs": "^3.2.1",
    "cross-env": "^7.0.2",
    "escape-html": "^1.0.3",
    "fast-diff": "^1.2.0",
    "firebase-admin": "9.3.0",
    "firebase-functions": "3.11.0",
    "immutable": "^4.0.0-rc.12",
    "is-hotkey": "^0.1.6",
    "normalize-url": "^5.3.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.4.3",
    "vue-petit-store": "^0.9.20",
    "vue-toasted": "^1.1.28"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@nuxt/typescript-build": "^2.0.3",
    "coffee-loader": "^1.0.0",
    "coffeescript": "^2.5.1",
    "html-loader": "^1.3.0",
    "jest": "^26.4.2",
    "json-loader": "^0.5.7",
    "nodemon": "^2.0.4",
    "pug": "^3.0.0",
    "pug-jest": "^1.0.1",
    "pug-plain-loader": "^1.0.0",
    "sass": "^1.41.1",
    "sass-loader": "10.0.2",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "vue-jest": "^3.0.7",
    "webpack-cli": "^3.3.12",
    "yaml-jest": "^1.0.5",
    "yaml-loader": "^0.6.0"
  }
}
